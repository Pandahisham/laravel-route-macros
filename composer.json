{
    "name": "brayniverse/laravel-router-macros",
    "description": "A collection of useful router macros.",
    "keywords": [
        "laravel",
        "router",
        "macros"
    ],
    "homepage": "https://github.com/brayniverse/laravel-router-macros",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher L Bray",
            "email": "chris@brayniverse.com",
            "homepage": "http://brayniverse.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.*",
        "illuminate/http": "5.1.* || 5.2.* || 5.3.* || 5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Brayniverse\\RouteMacros\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brayniverse\\RouteMacros\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
